<UserControl x:Class="ModernDiagnosticCenter.Admin.SettingsPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <mui:ModernTab Layout="List" SelectedSource="/Admin/Test.xaml">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="daily accounting" Source="/Admin/Test.xaml" />
                <mui:Link DisplayName="monthly accounting" Source="/Admin/MonthlyAccounting.xaml" />
                <mui:Link DisplayName="Test Name Setting" Source="/Admin/TestName.xaml" />
                <mui:Link DisplayName="Database" Source="/Admin/Test.xaml" />
                <mui:Link DisplayName="Test Name Setting" Source="/Admin/TestName.xaml" />
                <mui:Link DisplayName="Password And Username" Source="/Admin/AdminInfo.xaml" />
                
                
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
